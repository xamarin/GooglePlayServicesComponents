<enum-method-mappings>
	<!--
  This example changes the Java method:
    android.support.v4.app.Fragment.SavedState.writeToParcel (int flags)
  to be:
    android.support.v4.app.Fragment.SavedState.writeToParcel (Android.OS.ParcelableWriteFlags flags)
  when bound in C#.

  <mapping jni-class="android/support/v4/app/Fragment.SavedState">
    <method jni-name="writeToParcel" parameter="flags" clr-enum-type="Android.OS.ParcelableWriteFlags" />
  </mapping>

  Notes:
  - For Java interfaces, use "jni-interface" attribute instead of "jni-class" attribute.
  - To change the type of the return value, use "return" as the parameter name.
  - The parameter names will be p0, p1, ... unless you provide JavaDoc file in the project.
  -->
  <mapping jni-class="com/google/android/gms/vision/CameraSource$Builder">
    <method jni-name="setFacing" parameter="facing" clr-enum-type="Android.Gms.Vision.CameraFacing" />
  </mapping>

  <mapping jni-class="com/google/android/gms/vision/CameraSource">
    <method jni-name="getCameraFacing" parameter="return" clr-enum-type="Android.Gms.Vision.CameraFacing" />
  </mapping>

  <mapping jni-class="com/google/android/gms/vision/Frame$Metadata">
    <method jni-name="getRotation" parameter="return" clr-enum-type="Android.Gms.Vision.FrameRotation" />
  </mapping>

  <mapping jni-class="com/google/android/gms/vision/Frame$Builder">
    <method jni-name="setRotation" parameter="rotation" clr-enum-type="Android.Gms.Vision.FrameRotation" />
  </mapping>

</enum-method-mappings>
