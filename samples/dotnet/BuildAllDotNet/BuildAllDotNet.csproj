<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(_DefaultDotNetSampleTargetFrameworks)</TargetFrameworks>
    
    <!-- Some packages specify a minimum of 28 (eg: Xamarin.AndroidX.HeifWriter) -->
    <!--
    -->
    <SupportedOSPlatformVersion>29</SupportedOSPlatformVersion>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationId>com.companyname.BuildAllDotNet</ApplicationId>
    <ApplicationVersion>1</ApplicationVersion>
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>

   
    <!-- Override property from 'Directory.Build.props' -->
    <!--
    -->
    <AndroidGenerateResourceDesigner>true</AndroidGenerateResourceDesigner>

    <!-- Prevent linking, since that would remove ~everything -->
    <PublishTrimmed>False</PublishTrimmed>
    <RunAOTCompilation>False</RunAOTCompilation>
    <RootNamespace>BuildAllDotNet</RootNamespace>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Xamarin.AndroidX.Lifecycle.Runtime" Version="2.7.0.2" />
    <PackageReference Include="Xamarin.AndroidX.Lifecycle.ViewModel" Version="2.7.0.2" />
    <PackageReference Include="Xamarin.AndroidX.Lifecycle.Process" Version="2.7.0.2" />
    <PackageReference Include="Xamarin.AndroidX.Lifecycle.LiveData.Core" Version="2.7.0.2" />    
    <PackageReference Include="Xamarin.AndroidX.Annotation" Version="1.7.1.2" /> 
    <PackageReference Include="Xamarin.Kotlin.StdLib" Version="1.9.23.1" /> 
      
    <PackageReference Include="Xamarin.AndroidX.Fragment" Version="1.6.2.2" /> 
    <PackageReference Include="Xamarin.AndroidX.Collection" Version="1.4.0.1" /> 
    <PackageReference Include="Xamarin.AndroidX.Tracing.Tracing" Version="1.2.0.2" /> 
    <PackageReference Include="Xamarin.AndroidX.Core" Version="1.12.0.4" /> 
    <PackageReference Include="Xamarin.AndroidX.Activity" Version="1.8.2.1" /> 
    <PackageReference Include="Xamarin.KotlinX.Coroutines.Core" Version="1.8.0.2" /> 
    <PackageReference Include="Xamarin.AndroidX.Concurrent.Futures" Version="1.1.0.19" /> 
    <PackageReference Include="Xamarin.AndroidX.Concurrent.Futures" Version="1.1.0.10" /> 
    
    <PackageReference Include="Xamarin.AndroidX.AppCompat" Version="1.6.1.7" /> 
    <PackageReference Include="Xamarin.AndroidX.RecyclerView" Version="1.3.2.2" /> 
    <PackageReference Include="Xamarin.Google.Guava.ListenableFuture" Version="9999.0.0" /> 
    <PackageReference Include="Xamarin.AndroidX.Loader" Version="1.1.0.23" /> 
    <PackageReference Include="Xamarin.KotlinX.Coroutines.Android" Version="1.8.0.2" /> 
    <PackageReference Include="Xamarin.AndroidX.VectorDrawable.Animated" Version="1.1.0.23" /> 
  </ItemGroup>

