    <PackageReference Include="Xamarin.AndroidX.Browser" Version="1.8.0" />
    <PackageReference Include="Xamarin.AndroidX.Legacy.Support.V4" Version="1.0.0.23" />
    <PackageReference Include="Xamarin.Build.Download">
      <Version>0.11.4</Version>
    </PackageReference>
    <PackageReference Include="Xamarin.GooglePlayServices.Ads.Lite">
      <Version>122.3.0.3</Version>
    </PackageReference>
    <PackageReference Include="Xamarin.GooglePlayServices.Basement">
      <Version>117.6.0</Version>
    </PackageReference>
  </ItemGroup>
</Project>

