	<!-- Remove *zz* obfuscated classes, interfaces, methods, etc. -->
	<remove-node path="/api/package/class[contains(@name, 'zz') and @name != 'Api.zza' and @name != 'zzf' and @name != 'zzc' and @name != 'zzd' and @name != 'zze' and @name != 'zza' and @name != 'zzg']" />
